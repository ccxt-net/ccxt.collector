<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <AssemblyName>CCXT.Collector</AssemblyName>
        <RootNamespace>CCXT.Collector</RootNamespace>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>CCXT.Collector</PackageId>
        <Authors>SEONGAHN LEE</Authors>
        <Product>CCXT</Product>
        <Company>ODINSOFT</Company>
        <Description>CCXT.Collector – CryptoCurrency eXchange Data Collect Library for .NET</Description>
        <Copyright>Copyright OdinSoftware Co., Ltd. 2015~2024</Copyright>
        <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
        <PackageProjectUrl>https://github.com/ccxt-net/ccxt.collector</PackageProjectUrl>
        <PackageIcon>ccxt.net.api.png</PackageIcon>
        <PackageTags>cryptocurrency;exchange;trading;market-data;exchange-markets;bot;crypto</PackageTags>
        <PackageReleaseNotes>upgrade net8.0</PackageReleaseNotes>
        <Platforms>AnyCPU;x64;x86</Platforms>
        <Version>1.5.1</Version>
        <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
        <AssemblyVersion>1.5.1.13</AssemblyVersion>
		<FileVersion>1.5.1.13</FileVersion>
		<LangVersion>11.0</LangVersion>
		<ApplicationIcon>ccxt.net.ico</ApplicationIcon>
		<Title>CryptoCurrency eXchange Data Collect</Title>
		<RepositoryUrl>https://github.com/ccxt-net/ccxt.collector</RepositoryUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryType>git</RepositoryType>
	</PropertyGroup>

    <ItemGroup>
        <None Include="..\LICENSE.txt">
            <Pack>True</Pack>
            <PackagePath></PackagePath>
        </None>
        <None Include="..\logo-files\ccxt.net.api.png">
            <Pack>True</Pack>
            <PackagePath></PackagePath>
        </None>
        <None Include="..\README.md">
          <Pack>True</Pack>
          <PackagePath>\</PackagePath>
        </None>
        <Content Include="..\LICENSE.txt" Link="LICENSE.txt" />
        <Content Include="ccxt.net.ico" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="CCXT.NET" Version="1.5.1" />
        <PackageReference Include="Microsoft.Extensions.Configuration" Version="8.0.0" />
        <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="8.0.0" />
        <PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" Version="8.0.0" />
        <PackageReference Include="Microsoft.Extensions.Configuration.FileExtensions" Version="8.0.1" />
        <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="8.0.0" />
        <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
        <PackageReference Include="RabbitMQ.Client" Version="6.8.1" />
        <PackageReference Include="System.Text.Json" Version="8.0.5" />
        <PackageReference Include="System.ServiceModel.Primitives">
            <Version>8.0.0</Version>
        </PackageReference>
    </ItemGroup>

</Project>
