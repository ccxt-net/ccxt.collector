    <AssemblyName>ccxt.collector</AssemblyName>
    <RootNamespace>shared</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
